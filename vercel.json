{
    "builds": [
      {
        "src": "package/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "package/wsgi.py"
      }
    ]
}